<view class="cate-list"><block wx:if="{{cateObj.category}}"><swiper class="banner" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{cateObj.category.bannerUrlList}}" wx:for-item="bannerItem" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image class="cate-img" src="{{bannerItem}}" mode></image></view></swiper-item></block></swiper></block><view class="title">{{cateObj.category.name}}</view><view class="desc">{{cateObj.category.frontDesc}}</view><view class="shop-list"><block wx:for="{{cateObj.itemList}}" wx:for-item="shopItem" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['cateObj.itemList','id',shopItem.id]]]]]]]}}" class="shop-item" bindtap="__e"><image class="shop-list-img" src="{{shopItem.listPicUrl}}" mode></image><view class="desc">{{shopItem.name}}</view><view class="price">{{shopItem.retailPrice}}</view></view></block></view></view>