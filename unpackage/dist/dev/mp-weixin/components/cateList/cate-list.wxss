.cate-list .banner .cate-img {
  width: 100%;
}
.cate-list .title {
  font-size: 36rpx;
  color: #333;
  line-height: 70rpx;
  text-align: center;
}
.cate-list .desc {
  color: #999;
  font-size: 26rpx;
  line-height: 40rpx;
  text-align: center;
}
.cate-list .shop-list {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.cate-list .shop-list::after {
  content: "";
  display: block;
  width: 344rpx;
  height: 344rpx;
}
.cate-list .shop-list .shop-item {
  width: 344rpx;
}
.cate-list .shop-list .shop-item .shop-list-img {
  width: 344rpx;
  height: 344rpx;
}
.cate-list .shop-list .shop-item .desc {
  font-size: 26rpx;
  text-align: left;
  line-height: 50rpx;
}
.cate-list .shop-list .shop-item .price {
  font-weight: bold;
  color: #f40;
}

